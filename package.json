{
  "name": "pandorae",
  "version": "1.0.03",
  "description": "A document-centered research protocol",
  "license": "MIT",
  "author": "Guillaume Levrier",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Guillaume-Levrier/PANDORAE/"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "update": "git pull && npm i",
    "start": "electron .",
    "build:all": "cross-env NODE_ENV=production node bin/package.js",
    "package": "cd release/ && ls | while read d; do cd $d; rm -f *.zip; zip -r PANDORAE-$d.zip *; cd ..; done && mv */*.zip ."
  },
  "dependencies": {
    "@hownetworks/tracegraph": "^0.3.0",
    "@observablehq/runtime": "^4.8.2",
    "bottleneck": "^2.19.5",
    "csv-parser": "^3.0.0",
    "d3": "6.6.0",
    "dexie": "^3.2.2",
    "fastest-levenshtein": "^1.0.16",
    "keytar": "^7.9.0",
    "merge-files": "^0.1.2",
    "mnemonist": "^0.39.5",
    "npm": "^9.2.0",
    "postprocessing": "^6.30.1",
    "quill": "^1.3.7",
    "three": "0.150.1",
    "versor": "^0.1.2",
    "whois-json": "^2.0.4"
  },
  "devDependencies": {
    "cross-env": "^5.2.1",
    "del": "^4.1.1",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-packager": "^17.1.1",
    "electron-rebuild": "^2.3.4",
    "node-abi": "^2.19.3",
    "tiny-promisify": "^1.0.0",
    "webpack": "^4.44.0"
  },
  "build": {
    "artifactName": "PANDORAE.${ext}",
    "productName": "PANDORÃ†",
    "win": {
